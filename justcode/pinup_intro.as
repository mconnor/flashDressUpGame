/*coded May 2005by mike connor - info@rocketnumber9.com - http://rocketnumber9.comfor Agent 16this is a .AS file for a .Flaloader.fla loads intro.fla and layout.fla into emptymovieclips*/var my_date:Date = new Date();var monthNames_array:Array = new Array("January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December");var monthPinup_array:Array = new Array();var monthCount:Number;var startMonth:Number = 5;//junevar lastMonth:Number = 6;//julyvar ourSWF:MovieClip = this;// CLICK TO NEXT MONTHintroNav.nextMonth.onRelease = function() {	//ALL MONTH CODE	//if (monthCount >= monthNames_array.length) {	//monthCount = 0;	//}	//JUNE-JULY CODE- turn off next/last buttons on last	if (monthCount<=lastMonth) {		monthCount++;		introNav.monthAnim_mc.month.text = getMonthAsString(monthCount);		introNav.gotoAndPlay("nextMonthAnim");		introNav.next_mc2.gotoAndPlay("anim");		attachPinup();		if (monthCount == lastMonth) {			trace("turn OFF last butto becuzmonthCount is  "+monthCount);			introNav.nextMonth._visible = false;			introNav.next_mc2._visible = false;		}	}	//trace("prev button on? "+this.introNav.prev_mc._visible);	if (introNav.prev_mc2._visible == false) {		trace("turn prev button on");		introNav.prevMonth._visible = true;		introNav.prev_mc2._visible = true;	}	disableNoPinup();};//CLICK TO PREVIOUS MONTHintroNav.prevMonth.onRelease = function() {	if (monthCount>startMonth) {		monthCount--;		introNav.monthAnim_mc.month.text = getMonthAsString(monthCount);		introNav.gotoAndPlay("prevMonthAnim");		introNav.prev_mc2.gotoAndPlay("anim");		attachPinup();		if (monthCount == startMonth) {			trace("turn OFF last butto becuzmonthCount is  "+monthCount);			introNav.prevMonth._visible = false;			introNav.prev_mc2._visible = false;		}	}	if (introNav.next_mc2._visible == false) {		trace("turn NEXT button on");		introNav.nextMonth._visible = true;		introNav.next_mc2._visible = true;	}	disableNoPinup();};//turn off prev next buttons when needed// dump intro and slide in layoutintroNav.marker_mc.btn_goPinup.onRelease = function() {	if (monthPinup_array[monthCount] != "noPinup") {		trace("dump intro  "+monthPinup_array[monthCount]);		this._parent._parent._parent._visible = false;		//_level0.container_layout._x = 0; //this works too		//assign position o next movie		_parent.container_layout._x = 0;		_parent.container_layout.play();	//myInterval= setInterval(slideLayout,10);			//_level0.soundtrack.volume = 1;			trace("intro script-lowered volume "+_level0.soundtrack.volume);		swfVolume= setInterval(lowerVolume,100);	}};function disableNoPinup():Void {	trace("this months pinup " + monthPinup_array[monthCount]);	if (monthPinup_array[monthCount] == "noPinup") {		introNav.marker_mc.btn_goPinup.enabled=false;		trace("no pinup this moth, button disabled");	} else {		introNav.marker_mc.btn_goPinup.enabled=true;		trace("pinup this month. Button enabled");	}}function lowerVolume():Void {	_level0.soundtrack.volume = _level0.soundtrack.volume - 4;	if (_level0.soundtrack.volume<=40) {		clearInterval(swfVolume);		trace("sound faded out to  " + _level0.soundtrack.volume);	}}function slideLayout():Void {	currentX=_parent.container_layout._x;	_parent.container_layout._x = currentX - 5;	trace(currentX);	if (_parent.container_layout._x ==0) {		clearInterval(myInterval);	}	//updateAfterEvent();}//function init():Void {	trace("MONTH NUMBER IS "+my_date.getMonth());	//CODE FOR READING THE REAL MONTH IN	//monthCount = my_date.getMonth();	// CODE FOR STARTING MONTHIN JUNE. LAST MONTH WILL BE JULY	monthCount = 5;	//trace(getMonthAsString(my_date.getMonth()));	//monthList = ["June", "July", "August", "Septmeber", "December","January",];	this.introNav.month.text = getMonthAsString(monthCount);	setupPinUps();	attachPinup();	//introNav.girl.attachMovie(monthPinup_array[4], monthPinup_array[4], 1);	introNav.monthAnim_mc.month.text = getMonthAsString(monthCount);	trace("this is"+ourSWF);	//TURN OFF PREVIOUS BUTTONAT START	introNav.prevMonth._visible = false;	introNav.prev_mc2._visible = false;}function getMonthAsString(month:Number):String {	return monthNames_array[month];}function setupPinUps():Void {	 //assgin a pinUp to the corresponding month number.  Jan=0	monthPinup_array[4] = "noPinup";	monthPinup_array[5] = "lady1";	monthPinup_array[6] = "noPinup";	monthPinup_array[7] = "noPinup";}function attachPinup():Void {	trace("attach pinup");	trace("monthCount is "+monthCount);	//this if statement is not needed but works.  keep in case someone does not program a month in the seetupPinUps function	if (monthPinup_array[monthCount] == undefined) {		introNav.girl.attachMovie("noPinup", "noPinup", 1);		trace("no pinup this month");	} else {		trace("monthPinup_array[monthCount] is "+monthPinup_array[monthCount]);		introNav.girl.attachMovie(monthPinup_array[monthCount], monthPinup_array[monthCount], 1);	}}init();